{
  "name": "NuxtTemplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/device": "^1.2.7",
    "@nuxtjs/pwa": "^3.0.2",
    "@nuxtjs/style-resources": "^1.0.0",
    "core-js": "^3.6.5",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.14.5",
    "nuxt-i18n": "^6.20.1",
    "vuelidate": "^0.7.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/svg": "^0.1.12",
    "autoprefixer": "^9.8.6",
    "breakpoint-sass": "^2.7.1",
    "cssnano": "^4.1.10",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.16.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "husky": "^4.3.6",
    "postcss-sort-media-queries": "^1.7.26",
    "prettier": "^2.2.1",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.1.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "sort-css-media-queries": "^1.5.2",
    "stylelint": "^13.9.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./commitizen.js"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './commitlint.js'"
    }
  }
}
